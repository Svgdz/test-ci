{
  "name": "aibexx",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write .",
    "prepare": "husky",
    "scan:local": "npx react-scan@latest localhost:3000",
    "generate:supabase": "npx supabase@latest gen types typescript --schema public > src/types/database.types.ts --project-id $SUPABASE_PROJECT_ID",
    "scripts:check-app-env": "tsx scripts/check-app-env.ts",
    "scripts:check-e2e-env": "tsx scripts/check-e2e-env.ts",
    "scripts:check-all-env": "npm run scripts:check-app-env && npm run scripts:check-e2e-env",
    "scripts:create-migration": "tsx scripts/create-migration.ts",
    "shad": "npx shadcn@canary",
    "test": "vitest",
    "test:unit": "vitest run src/__test__/unit",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:chrome": "cypress run --browser chrome",
    "test:e2e:firefox": "cypress run --browser firefox",
    "test:e2e:webkit": "cypress run --browser webkit",
    "test:ci": "vitest run --coverage",
    "cypress:install": "cypress install",
    "lambdatest:run": "lambdatest-cypress run --config-file=lambdatest-config.json --cy=\"--config-file cypress.config.ts\"",
    "health-check": "tsx scripts/health-check.ts",
    "test:run": "npm run scripts:check-all-env && vitest run",
    "test:watch": "npm run scripts:check-all-env && vitest",
    "test:ui": "npm run scripts:check-all-env && vitest --ui",
    "test:ui:integration": "npm run scripts:check-app-env && vitest --ui src/__test__/unit/",
    "test:unit:components": "vitest run src/__test__/unit/components",
    "test:unit:utils": "vitest run src/__test__/unit/utils",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest --coverage --watch",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci:full": "npm run test:coverage && npm run test:e2e:chrome"
  },
